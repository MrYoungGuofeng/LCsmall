<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd ">
      <view class="newBox">
        <view class='lineBox'></view>基础信息
        <view class="msize"></view>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd " style='width:auto;'>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidths">
          <view class="weui-label">户 型：</view>
        </view>
        <picker class='inputsize' bindchange="bindCountryCodeChange" value="{{countryCodeIndex}}" data-chk='index' range="{{countryCodes}}">
          <view class="weui-input ">{{countryCodes[countryCodeIndex]}}</view>
        </picker>  
        <view style="width:20px; text-align:center;"> 房</view>
        <picker class='inputsize' bindchange="bindCountryCodeChange" value="{{countryCodeIndex}}" data-chk='index1' range="{{countryCodes}}">
          <view class="weui-input ">{{countryCodes[countryCodeIndex1]}}</view>
        </picker><view style="width:20px; text-align:center;"> 厅</view>  
        <picker class='inputsize' bindchange="bindCountryCodeChange" value="{{countryCodeIndex}}" data-chk='index2' range="{{countryCodes}}">
          <view class="weui-input ">{{countryCodes[countryCodeIndex2]}}</view>
        </picker> 
        <view style="width:20px; text-align:center;"> 厨</view>
        <picker class='inputsize' bindchange="bindCountryCodeChange" value="{{countryCodeIndex}}" data-chk='index3' range="{{countryCodes}}">
          <view class="weui-input ">{{countryCodes[countryCodeIndex3]}}</view>
        </picker>
        <view style="width:40px;text-align:center;"> 阳台</view>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd " style='width:auto;'>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidths">
          <view class="weui-label">装修公司：</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" placeholder="请输入电话号码" value="{{signDate}}" />
        </view>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd ">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidths">
          <view class="weui-label">装修风格：</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" placeholder="请输入qq" value="{{dectype}}" />
        </view>
      </view>
    </view>
    <view class="weui-cell__hd ">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidths">
          <view class="weui-label">面 积：</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" placeholder="请输入面积" value="{{measure}}" />
        </view>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd ">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidths">
          <view class="weui-label">合 同 价：</view>
        </view>
        <input  placeholder='填写合同价位' placeholder-style='color:#cccccc;'  />
          <text style='position:absolute; top:0;right:-50%'>10万含设计费</text>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd widthauto">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidth">
          <view class="weui-label checkBox">房屋类型：</view>
        </view>
        <checkbox-group bindchange="checkboxChange"  data-chk='decorationType' class="checkBox">
          <label class=" weui-check__label newWidth" wx:for="{{checkboxItems.decorationType}}" wx:key="value">
            <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}" />
            <view class="weui-cell__hd weui-check__hd_in-checkbox newcheckSzie">
              <icon class="weui-icon-radio" type="circle" size="12" wx:if="{{!item.checked}}"></icon>
              <icon class="weui-icon-radio" type="success" size="12" wx:if="{{item.checked}}"></icon>
            </view>
            <view class="weui-cell__bd">{{item.name}}</view>
          </label>
        </checkbox-group>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd widthauto">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidth">
          <view class="weui-label checkBox">装修情况：</view>
        </view>
        <checkbox-group bindchange="checkboxChange"  data-chk='decorationSituation' class="checkBox">
          <label class=" weui-check__label newWidth" wx:for="{{checkboxItems.decorationSituation}}" wx:key="value">
            <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}" />
            <view class="weui-cell__hd weui-check__hd_in-checkbox newcheckSzie">
              <icon class="weui-icon-radio" type="circle" size="12" wx:if="{{!item.checked}}"></icon>
              <icon class="weui-icon-radio" type="success" size="12" wx:if="{{item.checked}}"></icon>
            </view>
            <view class="weui-cell__bd">{{item.name}}</view>
          </label>
        </checkbox-group>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd widthauto">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidth">
          <view class="weui-label checkBox">装修用途：</view>
        </view>
        <checkbox-group bindchange="checkboxChange"  data-chk='decorationUse' class="checkBox">
          <label class=" weui-check__label newWidth" wx:for="{{checkboxItems.decorationUse}}" wx:key="value">
            <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}" />
            <view class="weui-cell__hd weui-check__hd_in-checkbox newcheckSzie">
              <icon class="weui-icon-radio" type="circle" size="12" wx:if="{{!item.checked}}"></icon>
              <icon class="weui-icon-radio" type="success" size="12" wx:if="{{item.checked}}"></icon>
            </view>
            <view class="weui-cell__bd">{{item.name}}</view>
          </label>
        </checkbox-group>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd widthauto">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidth">
          <view class="weui-label checkBox">装修形式：</view>
        </view>
        <checkbox-group bindchange="checkboxChange" data-chk='decorationForm' class="checkBox">
          <label class=" weui-check__label newWidth" wx:for="{{checkboxItems.decorationForm}}" wx:key="value">
            <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}" />
            <view class="weui-cell__hd weui-check__hd_in-checkbox newcheckSzie">
              <icon class="weui-icon-radio" type="circle" size="12" wx:if="{{!item.checked}}"></icon>
              <icon class="weui-icon-radio" type="success" size="12" wx:if="{{item.checked}}"></icon>
            </view>
            <view class="weui-cell__bd">{{item.name}}</view>
          </label>
        </checkbox-group>
      </view>
    </view>
  </view>
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__hd ">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd setWidths">
          <view class="weui-label">装修风格：</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" placeholder="请输入qq" value="{{dectype}}" />
        </view>
      </view>
    </view>
  </view>
</view>
<view class="weui-cells__title"></view>
<view class="weui-cells weui-cells_after-title setmarginbottom">
  <view class="weui-cell weui-cell_select borders">
    <view class="weui-cell__hd ">
      <view class="newBox">
        <view class='lineBox'></view>得房率信息(单位㎡)
        <view class="msize"></view>
        
      </view>
    </view>
    <view class='addtext' bindtap='istankuang'>
          添加
    </view>
  </view>
  <view class='listBox clearfloat'>
    <view class='boxSize' wx:for="{{roomRate}}">
      <view class='leftfloat'>{{item.name}}:</view>
      <input class='rightfloat' value='{{item.square}}'></input>
    </view>
  </view>
</view>
<view class='mask' wx:if='{{iskuang}}'>
  <view class="maskBox">
    <view class='maskTop'>添加得房率的信息</view>
    <view class='maskMiddle'>
     <view>
        <view class='content_detail'>
          <view class='half clearfloat'>
              <view class='name'>类型：</view>
              <input  placeholder='填写姓名' placeholder-style='color:#cccccc;'  />
          </view>
          <view class='half clearfloat'>
            <view class='name'>面积：</view>
            <input  placeholder='填写电话' placeholder-style='color:#cccccc;'  />㎡
          </view>
        </view>
    </view> 
    </view>
    <view class='maskfooter'>
        <view class='leftBox' bindtap='iskuangfrom'>确认</view>
        <view class='rightBox' bindtap='iskuangfrom'>取消</view>
    </view>
  </view>
</view>
<view class="weui-btn-area">
  <button class="weui-btn">确定</button>
</view>