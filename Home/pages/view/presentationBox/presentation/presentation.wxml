      <view class='leftBox leftSize'>
        <view class="{{currentTab==index?'sizeActive':''}}" data-current="{{index}}" bindtap="clickTab" wx:for="{{listInfo}}" data-currentTabId="{{item.id}}"  wx:key="{{index}}">{{item.serName}}</view>
