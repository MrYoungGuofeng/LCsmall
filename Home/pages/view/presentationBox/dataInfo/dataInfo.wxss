}
.weui-tab__panel .rightBox{
 width: 100%;
 height: 100%;
 float: left;
overflow-y:scroll;
max-height:900rpx;
padding-bottom: 1000rpx !important;
/* background: #f5f5f5; */
}

.weui-tab__panel{
  height: 100%;
  width:77%;
float:right;
}

.leftSize{
  border-right: 1px solid #ddd;
  text-align: center;
  font-size: 28rpx;
  width: 22%;
  float: left;
  margin-top:50px;
  height: 100%;
  /* box-shadow: 0rpx 10rpx 10rpx 1rpx #aaa; */
  /* height: 100%; */
}
.weui-bar__item_on{
  width: 100%
}
.leftSize view{
  border-bottom: 1px dashed #ddd;
  line-height: 40px;
  color: #000;
}
.leftSize view:last-child{
  border-bottom: none
}
.weui-tab__content{
  height: 100%;
  overflow: hidden;
}
.weui-tab__content:after{display:block;clear:both;content:"";visibility:hidden;height:0}
 view .sizeActive{
  color: #11b786 !important;
}
page,.page{
  overflow: hidden;
  height: 100%;
}
.weui-cell {
  padding: 10px 30rpx !important;
  box-sizing: border-box;
}
.listBox{
  border: 1px solid #ddd;
  margin:2.5%;
  overflow: hidden;
  border-radius: 10px;
}
.listBox:last-child{
  margin:4% 2.5% 8% 2.5%;
}
.weui-cell__bd,.weui-cell__ft{
  font-size: 24rpx;
}
.weui-cell__ft{
  float: right;
}
.buttonSize{
  height: 56rpx;
  line-height: 56rpx;
  font-size: 30rpx;
  background: #11b786 !important;
  padding: 0px 20rpx !important;
}
.listBoxmiddle:after{display:block;clear:both;content:"";visibility:hidden;height:0}
.imgBox,.userInfoBox{
  float: left;
  font-size: 28rpx;
  width: 100%  !important
}
.weui-btn{
  margin: 0;
}
.listBoxbottom{
  line-height: 20px;
}
.imgBox{
  width: 30%;
  text-align: center;
  line-height: 80px;
  padding-top: 30px;
}
.userInfoBox{
  width: 70%;
   font-size: 28rpx;
}
.imgBox image{
  width: 40px;
  height: 40px;
}
.textOver{
  width: 100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;
}
.linebox{
  width: 2px;
  height: 10px;
  background: red;
  display: inline-block;
  margin: 0 4px;
}
.listBoxmiddle{
  padding: 5px 40rpx;
  border-top:1px solid #ddd;
  border-bottom:1px solid #ddd;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #757575;
}
.weui-cell{
  padding: 5px 10px;
}
.textinfo{
  font-size: 32rpx;
  color: #11b786;
}
.textinfo text{
  font-size: 24rpx;
}
.iconfont{
  display: inline-block;
  margin-right: 5px;
}