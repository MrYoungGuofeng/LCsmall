.userImf{
  position: relative;
}
.userBox{
  height: 180rpx;
  background: #11b786;
}
page{
  background: #f5f5f5
}
.boxImg{
  position: absolute;
  width: 100px;
  height: 100px;
  bottom: -50px;
  background: #fff;
  border-radius: 50%;
  border:8px solid #fff;
  left:50%;
  margin-left: -54px; 

}
button::after{
border: none;
}
.weui-cell_access::after{
  border: none;
}
.boxImg image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  box-shadow: 4px 4px 10px #ddd;
}
.userText{
  height: 50px;
  padding-top: 100rpx;
  text-align: center;
  background: #ffffff;
  padding-bottom: 20rpx;
}
.marginBox{
  display: inline-block;
  height: 100%;
  margin: auto;
  text-align: center
}
.usrName{
  text-align: center;
  font-size: 40rpx;
}
.userInfo{
  display: block;
  text-align: center;
}
.userInfo text{
  display:inline-block;
  float:left;
  padding:0px 10rpx;
}

.line{
  height: 60%;
  margin: 4px;
  float: left;
  width: 1px;
  background: #ddd;
}

.lines{
  height: 60%;
  width: 1px;
  background: #ddd;
  position: absolute;
  top: 20%;
}
.green {
  color: #11b786;
}
.userInfo text{
  display: block;
   font-size: 12px;
}
.fleBox{
  text-align: center;
  position: relative;
  border-top:6px solid #f5f5f5;
  border-bottom:6px solid #f5f5f5;
  background: #fff;
  padding: 10px 0;
  color: #333
}
.weui-cell__bd{
  font-size: 32rpx;
  color: #333
}
.weui-flex__item{
  line-height: 20px;
}
.weui-flex__item text:nth-child(2){
  font-size: 24rpx;
  color:#666;
}
.fleBox text{
  display: block;
}
.iconfont{
  text-align: center;
  font-size: 32rpx;
  margin-right: 5px;
  vertical-align: middle;
  width:50rpx; 
  height: 50rpx;
  line-height: 50rpx;
}
#shareBtn{
  text-align:left;
  background:#fff;
  line-height:50rpx;
  border-color:#ffffff !important;
}
button{
  border: none !important;
}
.weui-cell_active{
  border: none !important;
}
.icon-add{font-size: 22px;color:#f5d69b;}
.icon-service{font-size: 22px;color:#518fec;}
.icon-share{color:#7cec54;} 
.icon-set{color:#075a43;}