/* 以下是按钮样式 */
.btns{
  margin-top:100rpx;
   position: fixed; 
  bottom:0rpx;
  width:100%;
  margin-bottom: 0rpx !important;
  padding-bottom: 0rpx !important;
  z-index:10;
}
.btns .weui-btn{
 font-size:30rpx;
 border-radius: 0;
 margin-top:0rpx;
 padding-top:8rpx;
 padding-bottom:8rpx;

}
.weui-btn[type='primary']{
 background-color: #11b786;
}
.weui-btn[type='warn']{
 background-color: #d7434d;
}
.overback{
 width:100%;
 height:100%;
 position:fixed;
 top:0;
 bottom:0;
 left:0;
 right:0;
  background: rgba(0, 0, 0, 0.5); 
 z-index: 999;
}
.dialog{
 width:70%;
 height:fit-content;
 position:absolute;
 top:200rpx;
 left:0;
 right:0;
 margin:auto;
 z-index: 1000;
 background-color: #fff;
 border-radius: 20rpx;
 font-size: 25rpx;
}
.dialog .header{
 padding:10rpx 20rpx;
 border-bottom: 1rpx solid #cccccc;
 color:#757575;
}
.dialog .dialog-content{
 padding: 20rpx;
}
.dialog .weui-textarea{
 width:460rpx;
 height:160rpx;
 background-color: #e5e5e5;
 padding: 10rpx;
}
.dialog .btngroup{
 display: flex;
 justify-content: center;
}
.dialog .btngroup view{
 text-align: center;
 font-size: 25rpx;
 margin-top:0rpx;
 color:#11b786;
 width:50%;
 padding-top:20rpx;
 padding-bottom: 20rpx;
 clear: both;
 border-top: 1rpx solid #cccccc;
}
.dialog .btngroup .certain{
 border-right: 1rpx solid #cccccc;
}
.borderstyle{
 width:50%;
 height:40rpx;
 border:1px solid #cccccc;
 position: absolute;
 top:0;
 left:110rpx;
}
.weui-label{
  width: 150rpx !important;
  text-align:right;
}
.weui-input{
  text-align: left;
}

.inputsize .weui-input{
border:1px solid #ddd;
width:160rpx !important;
min-height:18px;
float:left;
padding-left:10rpx;
position:relative;
height:20px;
line-height:20px;
border-radius:5px;
text-align:left;
letter-spacing:8rpx;

}
.weui-input{
  width: auto;
