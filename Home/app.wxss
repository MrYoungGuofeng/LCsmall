  /**app.wxss**/
@import 'weui.wxss';
@import 'icon.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.toast {  
  display: inline-block;  
  padding: 20rpx 40rpx;  
  min-width: 12000rpx;  
  font-size: 28rpx;  
  color: #fff;  
  background: rgba(0, 0, 0, .5);  
  border-radius: 10rpx;  
  text-align: center;  
} 
.zwxx{
  text-align: center;
  background: #fff;
  height: 100vh;
  padding-top: 200rpx
}
.zwxx image{
  width: 290rpx;
  height: 330rpx;
  opacity: 0.3;
}
.zwxx text{
  display: block;
  font-size: 28rpx;
  opacity: 0.3;
}
.baseline{
  text-align: center;
  color: #666;
  font-size: 28rpx;
  display: block;
  line-height: 80rpx;
  margin-top: -20rpx;
}